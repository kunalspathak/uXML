<?xml version="1.0"?>
<uXML language="Clike" scope="static">
	<block>
		<function name="factorial">
			<automatic-Var>
				<var mutable="yes" name="number" type="int" />
			</automatic-Var>
			<returns type="int" />
			<block>
				<operator value="==">
					<variable name="number" />
					<constant type="int" value="1" />
					<block>
						<return>
							<constant type="int" value="1" />
						</return>
					</block>
					<block>
						<return>
							<operator value="*">
								<variable name="number" />
								<call>
									<variable name="factorial" />
									<operator value="-">
										<variable name="number" />
										<constant type="int" value="1" />
									</operator>
								</call>
							</operator>
						</return>
					</block>
				</operator>
			</block>
		</function>
		<function name="main">
			<returns type="void" />
			<block>
				<print>
					<call>
						<variable name="factorial" />
						<constant type="int" value="5" />
					</call>
				</print>
			</block>
		</function>
		<call>
			<variable name="main" />
		</call>
	</block>
</uXML>