<?xml version="1.0"?>
<uXML language="Clike" scope="static">
	<block>
		<pointer-type name="int*" refType="int" />
		<var name="i" ptrMutable="yes" type="int*" valueMutable="yes" />
		<var mutable="yes" name="j" type="int">
			<constant type="int" value="5" />
		</var>
		<assign>
			<variable name="i" />
			<operator name="j" value="@">
				<variable name="j" />
			</operator>
		</assign>
		<print>
			<variable name="i" />
			<variable name="j" />
		</print>
	</block>
</uXML>