{
 Factorial of a number
}

PROGRAM procedure_call;
	var 
		i : int;
		a : array[3..4] of int;

begin
	for i := 3 to 4 do
	begin
		a[i] := i * i;
	end
	for i := 3 to 4 do
	begin
		write(a[i]);
	end
end

