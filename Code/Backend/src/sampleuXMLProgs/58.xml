<?xml version="1.0" encoding="UTF-8"?>
	<!--
		This program is same version of 50.xml program i.e. demonstration of
		the map function (string). However this one defines the function and use 
		the function name instead. 
	-->
<uXML xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xsi:noNamespaceSchemaLocation="../../../overall.xsd"
 language="Llike"
 scope="static">
	<!-- 
		(map function (one,two,three,four,five) ) 
		-->
	<block>
		<function name="concat">
			<automatic-Var>
				<var name="num" type="string" mutable="no" />
			</automatic-Var>
			<returns type="string" /> 
			<block>
				<return>
					<operator value="+">
						<variable name="num" />
						<constant type="string" value=" - brahmastra" />
					</operator>
				</return>
			</block>
		</function>


		<print>
			<call>
				<variable name="map" />
				<variable name="concat" />
				<constant type="string" value="one" />
				<constant type="string" value="two" />
				<constant type="string" value="three" />
				<constant type="string" value="four" />
				<constant type="string" value="five" />
			</call>
		</print>
	</block>
</uXML>
