<?xml version="1.0"?>
<uXML language="Ylike" scope="static" var-declaration="no">
	<block>
		<function name="fun">
			<automatic-Var>
				<var mutable="yes" name="a" type="Dynamic" />
			</automatic-Var>
			<block>
				<print>
					<variable name="a" />
				</print>
			</block>
		</function>
		<function name="fun1">
			<automatic-Var>
				<var mutable="yes" name="a" type="Dynamic" />
			</automatic-Var>
			<block>
				<call>
					<variable name="a" />
					<constant type="int" value="5" />
				</call>
			</block>
		</function>
		<call>
			<variable name="fun1" />
			<variable name="fun" />
		</call>
	</block>
</uXML>